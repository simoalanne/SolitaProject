{
  "name": "slt-boost",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "src/frontend",
    "src/backend",
    "src/shared"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace=src/frontend",
    "dev:backend": "npm run dev --workspace=src/backend",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "npm run build --workspace=src/frontend",
    "start": "npm run build && npm run start --workspace=src/backend",
    "start:docker": "docker build -t slt-boost . && docker run -p 3000:3000 slt-boost",
    "lint": "npm run lint --workspaces",
    "type-check": "npm run type-check --workspaces",
    "generate-business-finland-data": "cd src/setup && npm run generate && cd ../.."
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "playwright": "^1.56.1",
    "react-router-dom": "^6.30.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "concurrently": "9.2.1",
    "typescript": "^5.9.3"
  }
}
